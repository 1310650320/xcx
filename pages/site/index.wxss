page{width: 100%;background-color: #f6f6f6;}
.siteView{width: 100%;display: flex;flex-direction: column;}
.consigneeView{width: 100%;height: 100rpx;display: flex;flex-direction: row;justify-content: flex-start;border-bottom: 1rpx solid #f6f6f6;background-color: #ffffff;}
.left_title{width: 190rpx;height: 100rpx;margin-left: 30rpx;font-size: 29rpx;line-height: 100rpx;}
.text3{color: #ff9600;}
.right_title{height: 100rpx;font-size: 29rpx;line-height: 100rpx;}
.labelView{width: 100%;height: 100rpx;display: flex;flex-direction: row;align-items: center;margin-top: 20rpx;margin-bottom: 20rpx;}
.bottomLabel{background: #f6f6f6;line-height: 100rpx;font-size: 27rpx;color: #989898;padding-left: 30rpx;}
.btnView{width: 100%;height: 100rpx;display: flex;flex-direction: row;justify-content: center;align-items: center;margin-top: 20rpx;margin-bottom: 20rpx;}
.confirmBtn{width: 690rpx;background-color: #e4393c;color: #ffffff;}
.cancleBtn{width: 690rpx;background-color: #ffffff;color: #333333;}

.city-select-text{width: 100%;height: 100rpx;background: cyan;}



.listSiteView{margin-bottom: 105rpx;}
.listSite{display: flex;background-color: #ffffff;flex-direction: row;align-items: center;margin-bottom: 20rpx;}
.listSite_selV{width: 90rpx;height: 190rpx;display: flex;flex-direction: row;justify-content: center;align-items: center;}
.listSite_selV image{width: 36rpx;height: 36rpx;}
.siteList_RV{width: 80%;display: flex;flex-direction: column;}
.RV_top{width:600rpx;display: flex;padding-top: 20rpx;flex-direction: row;justify-content: space-between;}
.RV_top text{font-size: 29rpx;line-height: 50rpx;}  
.listSite image{width: 30rpx;height: 30rpx;}
.RV_text2{width:550rpx;font-size: 28rpx;padding-bottom: 20rpx;line-height: 50rpx;color: #848484;padding-right: 10rpx;}
.sitelistBto{width: 100%;position: fixed;bottom: 0rpx;background-color: #ffffff;height: 100rpx;line-height: 100rpx;text-align: center;font-size: 30rpx;color: red;border-top: 2rpx solid #f5f5f5;}
