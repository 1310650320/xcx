.formView{
  margin-top: 30rpx;
  padding: 10px;
}
.formView input{
  padding-right: 200rpx;
  padding-left: 20rpx;
  border-bottom: 1px solid #eee;
  margin-bottom: 40rpx;
  font-size: 16px;
}
.checkView{
  z-index: 99;
  position: absolute;
  top: 40rpx;
  right: 10rpx;
  font-size: 16px;
}
.applyBtn{
  margin-top: 100rpx;
  width: 88%;
  background-color:#f9c56f;
  border: none;
}
.blackview{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-color: black;
  opacity: .5;
  z-index:100;
}
.codeWrap{
  position: absolute;
  top: 200rpx;
  background-color: #fff;
  margin: 0 auto;
  width: 630rpx;
  margin-left: 50rpx;
  border-radius: 10rpx;
  z-index:999;
  margin-right: 50rpx;
  height:550rpx;
  border: 10rpx solid #e0e0e0;
}
.codetxt1{
  line-height:100rpx;
  height:100rpx;
  font-size:16px;
  margin-left:15px;
}
.codeline{
  width:90%;
  height:4rpx;
  background-color:#e0e0e0;
  margin:0 auto;
}
.codeinp{
  width: 120px;
  height: 30px;
  border: 2px solid #e0e0e0;
  margin: 0 auto;
  margin-top: 30px;
  font-size:16px;
  border-radius:5px;
  padding-left:10px;
}
.btnView{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top:10px;
}
.btn1{
  width: 250rpx;
  font-size: 16px;
  background-color: #f9c56f;
}
.btn2{
  width: 250rpx;
  font-size: 16px;
}
.codeImage{
  width:44%;
  height:110rpx;
  margin:0 auto;
  display:block;
  margin-top:40rpx;
  margin-bottom:30rpx;
}
.datepicker{
  color: rgb(128, 128, 128);
  padding-right:200rpx;
  padding-left:20rpx;
  border-bottom:1px solid #eee;
  margin-bottom:40rpx;
  font-size:16px;
}