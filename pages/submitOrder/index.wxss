.BtoView::before{
  display: block;
  height: 30rpx;
  width: 30rpx;
  content: "";
  position: absolute;
  clear: both;
  margin-left: -41rpx;
  margin-top: -15rpx;
  border-radius: 50%; 
  background-color: #fff;
  border-right:1rpx solid #e0e0e0;
}
.BtoView::after{
  display: block;
  height: 30rpx;
  width: 30rpx;
  content: "";
  position: absolute;
  clear: both;
  right: -16rpx;
  margin-top: -80rpx;
  border-radius: 50%; 
  background-color: #fff;
  border-left:1rpx solid #e0e0e0;
}
